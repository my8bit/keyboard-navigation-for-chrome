{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [{
      "css": ["navigate.css"],
      "js": ["jquery.js", "migemoDict.js", "migemo.js", "navigate.js", "hitahintmode.js"],
      "matches": ["http://*/*", "https://*/*", "file://*/*"],
      "run_at": "document_start"
   }],
