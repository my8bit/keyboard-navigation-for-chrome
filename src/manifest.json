{
   "background": {
      "page": "background.html"
   },
   "content_scripts": [{
      "css": ["navigate.css"],
      "js": ["app.js"],
      "matches": ["http://*/*", "https://*/*"],
      "run_at": "document_start"
   }],
   "web_accessible_resources": ["*"],
   "description": "Hit-a-Hint and migemo link search for Chrome",
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "manifest_version": 2,
   "name": "Keyboard-navigation-for-Chrome",
   "options_page": "options.html",
   "version": "0.8.1"
}