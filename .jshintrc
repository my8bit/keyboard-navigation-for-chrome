{
  // Details: https://github.com/victorporof/Sublime-JSHint#using-your-own-jshintrc-options
  // Example: https://github.com/jshint/jshint/blob/master/examples/.jshintrc
  // Documentation: http://www.jshint.com/docs/
  "browser": true,
  "esnext": true,
  "globals": { 
    "chrome": true, "console": true, //debugg variables
    "require": true, "requirejs": true, "define": true, //requirejs variables
    "GLOBAL": true, "process": true, "module": true, "exports": true, "__dirname": true, //Node.js internal
    "$": true,  //jQuery
    "describe": true, "beforeEach": true, "afterEach": true, "it": true //Mocha, Chai
  },
  "globalstrict": false,
  "quotmark": true,
  "undef": true,
  "unused": true
}
